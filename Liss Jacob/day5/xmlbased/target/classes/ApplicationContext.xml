<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="displayNameDI" class="com.demo.DI.DisplayNameUsingDI">
		<property name="name" value="Vijaya"></property>
	</bean>

	<bean id="displayNameConstructor"
		class="com.demo.ConStructorDI.DisplayNameUsingConstructorDI">
		<constructor-arg value="Vijaya"></constructor-arg>
	</bean>
	<bean id="displayData" class="com.demo.SetterDI.SetterDI">
		<property name="id">
			<value>10</value>
		</property>
		<property name="name">
			<value>Vijaya</value>
		</property>
	</bean>
</beans>